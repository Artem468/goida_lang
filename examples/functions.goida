функция факториал(n:число):число {
    если (n <= 1) {
        вернуть 1;
    } иначе {
        вернуть n * факториал(n - 1);
    }
}

а: число = 10.2;

печать(а);

функция максимум(a: число, b: число):число {
    если (a > b) {
        вернуть a;
    } иначе {
        вернуть b;
    }
}

печать(максимум);
печать(факториал(5));