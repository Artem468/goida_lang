печать("=== Тестирование списков, массивов и словарей ===");

печать("\n--- Тестирование типов ---");
л1 = список(1, 2, 3, 4, 5);
печать("Тип списка:", тип(л1));

м1 = массив(10, 20, 30);
печать("Тип массива:", тип(м1));

д1 = словарь("имя", "Алиса", "возраст", 25);
печать("Тип словаря:", тип(д1));

печать("\n--- Пустые коллекции ---");
л2 = список();
печать("Пустой список:", л2);
печать("Тип:", тип(л2));

м2 = массив();
печать("Пустой массив:", м2);
печать("Тип:", тип(м2));

д2 = словарь();
печать("Пустой словарь:", д2);
печать("Тип:", тип(д2));

печать("\n--- Проверка истинности ---");
если (список(1)) {
    печать("список(1) - истинен");
}

если (массив()) {
    печать("массив() - истинен");
} иначе {
    печать("массив() - ложен");
}

если (словарь("ключ", "значение")) {
    печать("словарь с элементами - истинен");
}

печать("\n--- Смешанные типы в коллекциях ---");
л3 = список(1, "текст", истина, 3.14);
печать("Смешанный список:", л3);

д3 = словарь("число", 42, "текст", "привет", "логический", ложь);
печать("Смешанный словарь:", д3);

печать("\n=== Все тесты пройдены успешно! ===");
