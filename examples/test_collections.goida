// Тестирование списков и словарей в языке Гойда

// Работа со списками
пусть числа = [1, 2, 3, 4, 5];
печать("Список чисел:");
печать(числа);

печать("Первый элемент: " + числа[0]);
печать("Третий элемент: " + числа[2]);

пусть имена = ["Алексей", "Борис", "Виктория"];
печать("Список имен:");
печать(имена);
печать("Второе имя: " + имена[1]);

// Работа со словарями
пусть возрасты = {
    "Алексей": 25,
    "Борис": 30,
    "Виктория": 28
};

печать("Словарь возрастов:");
печать(возрасты);

печать("Возраст Алексея: " + возрасты["Алексей"]);
печать("Возраст Виктории: " + возрасты["Виктория"]);

// Комбинированная работа - список словарей
пусть студенты = [
    {"имя": "Анна", "возраст": 20, "оценка": 5},
    {"имя": "Борис", "возраст": 21, "оценка": 4}
];

печать("Список студентов:");
печать(студенты);

пусть первый_студент = студенты[0];
печать("Первый студент: " + первый_студент["имя"]);
печать("Его возраст: " + первый_студент["возраст"]);
печать("Его оценка: " + первый_студент["оценка"]);

// Словарь со списком
пусть группы = {
    "первая": [1, 2, 3],
    "вторая": [4, 5, 6]
};

печать("Словарь групп:");
печать(группы);
печать("Первая группа: " + группы["первая"]);
печать("Первый элемент первой группы: " + группы["первая"][0]);
