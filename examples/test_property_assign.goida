класс Человек {
    публичный имя: текст;
    публичный активен: логический;
    публичный возраст: число;
    
    конструктор создать(имя_параметр: текст, возраст_параметр: число) {
        это.имя = имя_параметр;
        это.возраст = возраст_параметр;
        это.активен = истина;
    }
    
    публичный функция получить_имя() -> текст {
        вернуть это.имя;
    }
}

человек = новый Человек("Иван", 25);
печать("Имя:");
печать(человек.получить_имя());
печать("Активен:");
печать(человек.активен);
человек.активен = ложь;
печать("После изменения:");
печать(человек.активен);
