// Простой тест циклов
н = 7;

если (н > 5) {
    печать("Число больше 5");
}

// Цикл while
счетчик = 0;
печать("Цикл while:");
пока (счетчик < 3) {
    печать(счетчик);
    счетчик = счетчик + 1;
}

функция проверка(f: число): логический {
    если (f < 4){
        вернуть истина;
    } иначе {
        вернуть ложь;
    }
}


// Цикл for
печать("Цикл for:");
для (i = 1; проверка(i); i += 1) {
    печать(i);
}
