подключить "math_module";

// Тестируем создание класса из импортированного модуля
пусть точка = новый math_module.Точка(3, 4);
печать("Создали точку с координатами x=" + точка.х + ", y=" + точка.у);
печать("Расстояние до центра: " + точка.расстояние_до_центра());

пусть вектор = новый math_module.Вектор(5);
печать("Создали вектор с длиной: " + вектор.длина);
