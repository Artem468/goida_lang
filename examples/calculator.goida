// Простой калькулятор
пусть a:число = 10;
пусть b:число = 5;

печать("a = " + a);
печать("b = " + b);

печать("a + b = " + (a + b));
печать("a - b = " + (a - b));
печать("a * b = " + (a * b));
печать("a / b = " + (a / b));
печать("a % b = " + (a % b));

// Логические операции
пусть большеЧем:логический = a > b;
пусть равно:логический = a == b;

печать("a > b: " + большеЧем);
печать("a == b: " + равно);
