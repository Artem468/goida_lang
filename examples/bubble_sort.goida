пусть список1 = [4, 6, 2, 7, 10, 567, 123, 25, 32, 64, 76, 15];
пусть длина = размер(список1);

для (и = 0; длина - 1) {
    для (ж = 0; длина - 2 - и) {
        если (список1[ж] > список1[ж + 1]) {
            вр = список1[ж];
            список1[ж] = список1[ж + 1];
            список1[ж + 1] = вр;
        }
    }
}

печать(список1);