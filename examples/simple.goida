функция применить(ф: функция[(число, число): число], а: число, б: число): число {
    вернуть ф(а, б);
}

функция сложить(х: число, у: число): число {
    вернуть х + у;
}

функция печать_результата() {
    пусть результат = применить(сложить, 5, 7);
    печать("Результат: " + результат);
}

печать_результата();