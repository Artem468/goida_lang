класс Тест {
    публичный х: число;
    публичный у: число;
    
    конструктор создать(а: число, б: число) {
        это.х = а;
        это.у = б;
        печать("В конструкторе - это.х:");
        печать(это.х);
        печать("В конструкторе - это.у:");
        печать(это.у);
    }
}

печать("Создаём объект...");
т = новый Тест(10, 20);
печать("После создания - т.х:");
печать(т.х);
печать("После создания - т.у:");
печать(т.у);
