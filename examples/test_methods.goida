// Тестирование методов для работы со списками и словарями

// Работа со списками
пусть список1 = [1, 2, 3];
печать("Исходный список1: " + список1);
печать("Размер списка: " + длина(список1));

// Добавляем элемент
добавить(список1, 4);
печать("После добавления 4: " + список1);

// Проверяем содержание
печать("Содержит 4: " + содержит(список1, 4));
печать("Содержит 10: " + содержит(список1, 10));

// Извлекаем последний элемент
пусть последний = извлечь(список1);
печать("Извлеченный элемент: " + последний);
печать("список1 после извлечения: " + список1);

// Удаляем элемент по индексу
удалить(список1, 1);
печать("После удаления элемента с индексом 1: " + список1);

// Работа со словарями
пусть словарь1 = {"a": 1, "b": 2};
печать("Исходный словарь1: " + словарь1);
печать("Размер словаря: " + длина(словарь1));


// Проверяем содержание ключей
печать("Содержит ключ 'a': " + содержит(словарь1, "a"));
печать("Содержит ключ 'z': " + содержит(словарь1, "z"));

// Добавляем новый элемент в словарь1
добавить(словарь1, "c", 3);
печать("После добавления ключа 'c': " + словарь1);
печать("Размер после добавления: " + длина(словарь1));

// Перезаписываем существующий ключ
добавить(словарь1, "b", 22);
печать("После изменения значения ключа 'b': " + словарь1);

// Удаляем ключ
пусть удаленное_значение = удалить(словарь1, "a");
печать("Удаленное значение: " + удаленное_значение);
печать("словарь1 после удаления ключа 'a': " + словарь1);

печать("Финальный размер словаря: " + длина(словарь1));
