печать("=== Тестирование методов коллекций ===");

печать("\n--- Список: добавить и получить ---");
л = список(1, 2, 3);
печать("Исходный список:", л);
печать("Длина:", длина(л));

добавить(л, 4);
добавить(л, 5);
печать("После добавления 4 и 5:", л);

печать("Получить элемент 0:", получить(л, 0));
печать("Получить элемент 2:", получить(л, 2));
печать("Получить элемент -1 (последний):", получить(л, -1));

печать("\n--- Список: изменить ---");
старое = изменить(л, 1, 100);
печать("Старое значение на позиции 1:", старое);
печать("После изменения:", л);

печать("\n--- Список: удалить ---");
удаленное = удалить(л, 2);
печать("Удалено значение:", удаленное);
печать("После удаления:", л);

печать("\n--- Массив: неизменяемый ---");
м = массив(10, 20, 30);
печать("Массив:", м);
печать("Длина:", длина(м));
печать("Получить элемент 1:", получить(м, 1));

печать("\n--- Словарь: работа с методами ---");
д = словарь("имя", "Алиса", "возраст", 25);
печать("Исходный словарь:", д);
печать("Длина:", длина(д));

печать("Получить по ключу 'имя':", получить(д, "имя"));
печать("Получить по ключу 'возраст':", получить(д, "возраст"));

печать("\n--- Словарь: добавить ---");
добавить(д, "город", "Москва");
печать("После добавления города:", д);

печать("\n--- Словарь: изменить ---");
старое_возраст = изменить(д, "возраст", 26);
печать("Старый возраст:", старое_возраст);
печать("После изменения:", д);

печать("\n--- Словарь: удалить ---");
удаленный_город = удалить(д, "город");
печать("Удалено значение города:", удаленный_город);
печать("После удаления:", д);

печать("\n--- Ключи и значения ---");
д2 = словарь("первый", 1, "второй", 2, "третий", 3);
печать("Словарь:", д2);
печать("Ключи:", ключи(д2));
печать("Значения:", значения(д2));

печать("\n=== Все тесты методов пройдены! ===");
